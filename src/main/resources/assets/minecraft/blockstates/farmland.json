        {   "apply": { "model": "block/farmland" }},
        {   "when": { "moisture": "7" },
            "apply": { "model": "block/farmland_moist" }
        },
        {   "when": { "fertilized": "true" },
            "apply": { "model": "block/farmland_fertilized" }
        }
    ]
}
